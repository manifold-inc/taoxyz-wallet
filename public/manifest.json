{
  "manifest_version": 3,
  "name": "Taoxyz Wallet",
  "version": "1.0.0",
  "description": "",
  "permissions": [],
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "public/tao.png",
      "48": "public/tao.png",
      "128": "public/tao.png"
    }
  },
  "background": {
    "service_worker": "background/background.js"
  },
  "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content/content.js"]
    }
  ]
}
